notas = [8.5, 4.0, 10.0, 7.5, 6.0, 9.0]

soma = 0          # para acumular o total das notas
contador_boas = 0 # para contar quantas notas >= 7.0

for nota in notas:
    soma += nota
    if nota >= 7.0:
        contador_boas += 1

media = soma / len(notas)

print("MÃ©dia das notas:", media)
print("Quantidade de notas >= 7.0:", contador_boas)
